<div class="container">
 <div class="row ">
   <div class="col s12 m12 l12 center">
    <h1 class="orbitron">Configurações</h1>
   	
   </div>






   <div class="row">
   	<%= form_tag(choose_station_path, :method=>'post') do %>
     
    <div class="col s12 m10 l10">
     <div class="input-field">
     <%= select_tag("station", options_for_select([['Estação1', '1'],
                 ['Estação2', '2']], '2')) %>
      <label>Estação</label>
     </div>
    </div>
    

    <div class="col s2 m2 l2">
     <div class="input-field">
      <%= submit_tag 'Ok' %> 
     </div>
    </div>
   
    
    <%end%>
  </div>
  


 	
 </div>

	


</div>




























